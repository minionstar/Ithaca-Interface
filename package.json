{
  "name": "ithaca-interface",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "patch": "patch-package",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "checks:run:fix": "yarn lint:fix && yarn format:fix",
    "check:prod:build": "yarn build && yarn start",
    "find-deadcode": "ts-prune"
  },
  "dependencies": {
    "@ithaca-finance/sdk": "5.2.1",
    "@radix-ui/react-popover": "1.0.7",
    "@rainbow-me/rainbowkit": "^1.1.3",
    "@sentry/nextjs": "^7.105.0",
    "@tanstack/react-query": "^5.28.14",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.4",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "framer-motion": "^10.16.4",
    "mixpanel-browser": "^2.49.0",
    "next": "14.1.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.38",
    "react": "18.3.0",
    "react-csv": "^2.2.2",
    "react-day-picker": "^8.9.1",
    "react-dom": "18.3.0",
